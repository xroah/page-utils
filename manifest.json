{
  "manifest_version": 2,
  "name": "页面工具",
  "version": "2.0.0",
  "description": "页面工具",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "页面工具",
    "default_icon": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "sessions",
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["scripts/gesture.js"],
      "css": ["styles/gesture.css"],
      "run_at": "document_start"
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "options_page": "options.html",
  "web_accessible_resources": ["images/*", "icons/*"]
}